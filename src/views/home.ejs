<%- include partials/header.ejs %>

<p>Book Store</p>

<% books.forEach((book) => { %>
  <div class="row">
    <a href="/book/<%= book.id %>"><p><%= book.title %></p></a>
    <p><%= book.author %></p>
    <p><%= book.genre.replace("_", " ") %></p>
    <p><%= book.pages %></p>
    <a href='/delete/<%= book.id %>'><button>delete</button></a>
  </div>
<% })%>

<%- include partials/footer.ejs %>
